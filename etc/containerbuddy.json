{
  "consul": "{{ .CONSUL }}:8500",
  "services": [
    {
      "name": "couchbase-api",
      "port": 8092,
      "health": ["/usr/local/bin/manage.sh", "health"],
      "poll": 5,
      "ttl": 15
    },
    {
      "name": "couchbase-web",
      "port": 8091,
      "health": ["curl", "-Ls", "-o", "/dev/null", "--fail", "http://localhost:8091"],
      "poll": 5,
      "ttl": 15
    }
  ]
}
